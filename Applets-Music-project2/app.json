{
    "lazyCodeLoading": "requiredComponents",
    "pages": [
        "pages/main-music/main-music",
        "pages/main-video/main-video"
    ],
    "subPackages": [
        {
            "root": "packagePlayer",
            "pages": [
                "pages/music-player/music-player"
            ]
        },
        {
            "root": "packageVideo",
            "pages": [
                "pages/detail-video/detail-video"
            ]
        },
        {
            "root": "packageSearch",
            "pages": [
                "pages/detail-search/detail-search"
            ]
        },
        {
            "root": "packageUser",
            "pages": [
                "pages/user-message/user-message"
            ]
        },
        {
            "root": "packageRollvide",
            "pages": [
                "pages/detail-rollvide/detail-rollvide"
            ]
        },
        {
            "root": "packageSong",
            "pages": [
                "pages/detail-song/detail-song"
            ]
        }
        ,
        {
            "root": "packageMenu",
            "pages": [
                "pages/detail-menu/detail-menu"
            ]
        }
    ],
    "preloadRule": {
        "pages/main-music/main-music": {
            "network": "all",
            "packages": [
                "packagePlayer",
                "packageSearch",
                "packageSong"
            ]
        },
        "pages/main-video/main-video": {
            "network": "all",
            "packages": [
                "packageVideo"
            ]
        },
        "packageSearch/pages/detail-search/detail-search":{
            "network": "all",
            "packages": [
                "packageUser",
                "packageRollvide"
            ]
        }
    },
    "requiredBackgroundModes": [
        "audio",
        "backgroundAudioManager"
    ],
    "window": {
        "backgroundTextStyle": "dark",
        "navigationBarBackgroundColor": "#fff",
        "navigationBarTitleText": "Applets-Music-project",
        "navigationBarTextStyle": "black"
    },
    "tabBar": {
        "color": "#95969b",
        "selectedColor": "#fd3446",
        "list": [
            {
                "pagePath": "pages/main-music/main-music",
                "text": "音乐",
                "iconPath": "assets/images/tabbar/Music_active.png",
                "selectedIconPath": "assets/images/tabbar/Music.png"
            },
            {
                "pagePath": "pages/main-video/main-video",
                "text": "视频",
                "iconPath": "assets/images/tabbar/Video.png",
                "selectedIconPath": "assets/images/tabbar/Video_active.png"
            }
        ]
    },
    "style": "v2",
    "sitemapLocation": "sitemap.json"
}